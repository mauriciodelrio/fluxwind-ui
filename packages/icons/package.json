{
  "name": "@fluxwind/icons",
  "version": "0.0.0",
  "description": "Icon system for Fluxwind UI - Lucide icons wrapper",
  "keywords": [
    "icons",
    "lucide",
    "react",
    "ui",
    "fluxwind"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --config ../../eslint.config.cjs",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "lucide-react": "^0.553.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.7"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
