{
  "name": "@fluxwind/playground",
  "version": "0.0.0",
  "private": true,
  "description": "Playground and testing environment for Fluxwind UI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --config ../../eslint.config.cjs",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@fluxwind/core": "workspace:*",
    "@fluxwind/themes": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "tailwindcss": "3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.1"
  }
}
